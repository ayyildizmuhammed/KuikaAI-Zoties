{
    "name": "motus-web",
    "version": "1.0.0",
    "description": "Motus Web",
    "main": "index.ts",
    "scripts": {
        "dev": "concurrently \"nodemon --exec ts-node index.ts\" \"cd SPA && npm run dev\"",
        "server": "ts-node index.ts",
        "build:server": "cd Server && npm install && tsc",
        "build:client": "cd SPA && npm install && npm run build",
        "build": "npm run build:server && npm run build:client",
        "start": "node dist/index.js",
        "client": "cd SPA && npm run dev",
        "format": "prettier --write ."
    },
